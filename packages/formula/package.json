{
  "name": "@umbrajs/formula",
  "version": "0.1.0",
  "description": "A powerful Vue 3 form library with change tracking, diffing, and Zod validation",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "vue",
    "vue3",
    "form",
    "forms",
    "validation",
    "zod",
    "change-tracking",
    "diff",
    "reactive"
  ],
  "peerDependencies": {
    "vue": "^3.5.24",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@types/node": "24.10.1",
    "typescript": "^5.9.3",
    "vite": "7.2.2",
    "vue": "^3.5.24",
    "zod": "4.1.12"
  }
}
