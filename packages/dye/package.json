{
  "name": "@umbrajs/dye",
  "type": "module",
  "version": "0.0.155",
  "private": false,
  "description": "Vue color picker using umbra",
  "author": "Samuel M. Bednarz<https://github.com/CarelessCourage>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/umbrajs/dye"
  },
  "keywords": [
    "vue",
    "color",
    "picker",
    "umbra"
  ],
  "exports": {
    ".": {
      "import": "./dist/dye.js",
      "require": "./dist/dye.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/dye.umd.cjs",
  "module": "./dist/dye.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "style": "./dist/style.css",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "types": "vue-tsc ",
    "preview": "vite preview"
  },
  "dependencies": {
    "@umbrajs/core": "catalog:",
    "@vueuse/components": "catalog:",
    "@vueuse/core": "catalog:",
    "colord": "catalog:",
    "nearest-color": "catalog:",
    "pinia": "catalog:",
    "vue": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "npm-run-all": "catalog:",
    "sass": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vue-tsc": "catalog:"
  }
}
