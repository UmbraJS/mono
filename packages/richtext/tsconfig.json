{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "umbraco": [
        "../umbraco/index.ts"
      ],
      "umbraco/*": [
        "../umbraco/*"
      ]
    },
    // ...
    "allowJs": true,
    "noFallthroughCasesInSwitch": true,
    "declaration": true,
    "declarationDir": "dist",
    "outDir": "dist",
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "./**/*.ts",
    "./**/*.d.ts",
    "./**/*.vue",
    "index.ts"
  ],
  "exclude": [
    "./**/__tests__/*"
  ]
}
