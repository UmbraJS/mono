{
  "name": "@umbrajs/dye",
  "private": false,
  "version": "0.0.155",
  "description": "Vue color picker using umbra",
  "author": "Samuel M. Bednarz<https://github.com/CarelessCourage>",
  "license": "MIT",
  "keywords": [
    "vue",
    "color",
    "picker",
    "umbra"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/umbrajs/dye"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/dye.umd.cjs",
  "module": "./dist/dye.js",
  "types": "./dist/types/index.d.ts",
  "style": "./dist/style.css",
  "exports": {
    ".": {
      "import": "./dist/dye.js",
      "require": "./dist/dye.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "types": "vue-tsc ",
    "preview": "vite preview"
  },
  "dependencies": {
    "@umbrajs/core": "^0.0.456",
    "@vueuse/components": "12.7.0",
    "@vueuse/core": "12.7.0",
    "colord": "^2.9.3",
    "nearest-color": "^0.4.4",
    "pinia": "3.0.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/node": "22.13.4",
    "@vitejs/plugin-vue": "5.2.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.85.0",
    "typescript": "5.7.3",
    "vite": "6.1.1",
    "vite-plugin-dts": "4.5.0",
    "vue-tsc": "2.2.2"
  }
}
